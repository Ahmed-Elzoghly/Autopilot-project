\newcommand*{\blankpage}{%
  \begin{otherlanguage}{english}
    \vspace*{\fill}
%    \hfill
    \begin{center}
      \emph{This page is intentionally left blank!}
    \end{center}
    \vspace{\fill}
%    \thispagestyle{empty}
  \end{otherlanguage}
  \clearpage
}

\newcommand*\plainClearToLeftPage{%
  \clearpage
  \ifodd\value{page}

    \vspace*{\fill}
%    \hfill
    \vspace{\fill}

  \fi
}

%A more complex version that supports two-column
%\makeatletter
